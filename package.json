{
  "name": "@upsub/client",
  "description": "",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "test": "npm run lint && jest --coverage",
    "test:watch": "jest --watch",
    "lint": "standard | snazzy; exit 0",
    "lint:fix": "standard --fix | snazzy; exit 0",
    "build": "babel src/ -D -d ./lib; exit 0",
    "build:watch": "babel src/ -D -d ./lib --watch",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build && npm test"
  },
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "module"
  ],
  "author": {
    "name": "Frederik Kvartborg Albertsen",
    "email": "hello@kvartborg.net",
    "url": "https://kvartborg.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/upsub/client-js"
  },
  "bugs": {
    "url": "https://github.com/upsub/client-js/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "coveralls": "^3.0.0",
    "jest": "^22.1.1",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-watch": "^4.3.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "coveragePathIgnorePatterns" : ["node_modules/", "test/util/"]
  },
  "dependencies": {
    "ws": "^4.0.0"
  }
}
